<Window x:Class="KinoVideoProkat_K.Windows.VirychkaPDF"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KinoVideoProkat_K.Windows"
        mc:Ignorable="d"
        Title="VirychkaPDF" Height="200" Width="200">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="0 0 0 10">
                <TextBlock Text="с:" />
                <TextBox Name="TbViruchkaS" Width="100"/>
            </StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="0 0 0 10">
                <TextBlock Text="по:" />
                <TextBox Name="TbViruchkaP" Width="100"/>
            </StackPanel>

            <Button Name="BtnPDF" Click="BtnPDF_Click" Content="Сформировать PDF" />
        </StackPanel>
    </Grid>
</Window>
